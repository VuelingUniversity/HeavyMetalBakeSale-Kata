@model List<MetalBake.core.Models.ItemStock>
@{
    ViewBag.Title = "Index";
}

<h2>Inventario</h2>

<table style="width:200px">
    <tr>
        <th>Producto</th>
        <th>Stock</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ItemId</td>
            <td>@item.Stock</td>
        </tr>
    }
</table>