@model List<MetalBake.Web.Models.ItemStock>
@{
    ViewBag.Title = "Index";
}

<h2>Inventario</h2>
<table class="table">
    <tr>
        <th>Producto</th>
        <th>Stock</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ItemId</td>
            <td>@item.Stock</td>
            <td>@Html.ActionLink("Editar stock", "Edit", "Stock", new { itemId = item.ItemId }, null)</td>
        </tr>
    }
</table>